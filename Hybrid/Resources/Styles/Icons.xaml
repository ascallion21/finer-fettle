<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Hybrid">

    <FontImage x:Key="IconOne"
               FontFamily="FontAwesome"
               Size="22"
               Color="{StaticResource Primary}"
               Glyph="{x:Static local:IconFont.DiceOne}"/>
    <FontImage x:Key="IconTwo"
               FontFamily="FontAwesome"
               Size="22"
               Color="{StaticResource Primary}"
               Glyph="{x:Static local:IconFont.DiceTwo}"/>
    <FontImage x:Key="IconThree"
               FontFamily="FontAwesome"
               Size="22"
               Color="{StaticResource Primary}"
               Glyph="{x:Static local:IconFont.DiceThree}"/>
    <FontImage x:Key="IconFour"
               FontFamily="FontAwesome"
               Size="22"
               Color="{StaticResource Primary}"
               Glyph="{x:Static local:IconFont.DiceFour}"/>

</ResourceDictionary>