  <ItemGroup>
    <InternalsVisibleTo Include="Web.Test" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Code\TagHelpers\" />
    <Folder Include="Migrations\" />
    <Folder Include="wwwroot\css\" />
