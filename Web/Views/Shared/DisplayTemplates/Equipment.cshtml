@model Lib.ViewModels.Newsletter.EquipmentViewModel
@using Core.Models.Exercise

@Html.Raw(String.Join(", ", Model.UserEquipment
    .Select(e => e.Name)
    .Concat(Model.AllEquipment
        .Except(Model.UserEquipment)
        .Select(e => $"<span style='color:darkred'>{e.Name}</span>")
    )
))
