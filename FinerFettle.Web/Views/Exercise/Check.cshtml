@model ViewModels.Exercise.CheckViewModel
@using Models.Exercise
@using Models.Newsletter
@using Extensions
@using Controllers

@{
    ViewData["Title"] = "Exercises";
    Layout = "_Blank";
}

<div>
    <dl>
        <dt>Missing 100% progression range:</dt>
        <dd>
            @(String.Join(", ", Model.Missing100PProgressionRange))
        </dd>

        <dt>Missing Rep Range:</dt>
        <dd>
            @(String.Join(", ", Model.MissingRepRange))
        </dd>

        <dt>Missing Proficiency:</dt>
        <dd>
            @(String.Join(", ", Model.MissingProficiency))
        </dd>
    </dl>
</div>
